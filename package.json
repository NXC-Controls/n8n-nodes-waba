{
  "name": "n8n-nodes-waba",
  "version": "0.1.6",
  "description": "n8n node for WABA messaging service",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "waba",
    "whatsapp",
    "messaging"
  ],
  "license": "MIT",
  "homepage": "https://github.com/NXC-Controls/n8n-nodes-waba.git",
  "author": {
    "name": "NXC Controls Pvt Ltd",
    "email": "nxc.controls@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NXC-Controls/n8n-nodes-waba.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WabaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Waba/Waba.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "dependencies": {}
